{% extends 'base.html.twig' %}

{% block title %}Channel{% endblock %}

{% block body %}
    <h1>Channel</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ channel.id }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ channel.url }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ channel.title }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ channel.updatedAt ? channel.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ channel.createdAt ? channel.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>ErrorNb</th>
                <td>{{ channel.errorNb }}</td>
            </tr>
            <tr>
                <th>CallNb</th>
                <td>{{ channel.callNb }}</td>
            </tr>
            <tr>
                <th>Active</th>
                <td>{{ channel.active ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('channel_index') }}">back to list</a>

    <a href="{{ path('channel_edit', {'id': channel.id}) }}">edit</a>

    {{ include('channel/_delete_form.html.twig') }}
{% endblock %}
